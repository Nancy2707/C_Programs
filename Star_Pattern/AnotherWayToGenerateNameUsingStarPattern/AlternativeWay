void printPattern(char ch) {
    if (ch >= 'A' && ch <= 'Z') {
        int index = ch - 'A';
        for (int i = 0; i < ROWS; i++) {
            for (int j = 0; j < COLS; j++) {
                printf("%c", patterns[index][i][j]);
            }
            printf("\n");
        }
    }
}

void printNamePattern(const char* name) {
    for (int i = 0; i < strlen(name); i++) {
        if (name[i] >= 'A' && name[i] <= 'Z') {
            printPattern(name[i]);
            printf("\n"); // Separate each letter by a new line
        }
    }
}

int main() {
    char name[100];
    printf("Enter your name in uppercase: ");
    scanf("%s", name);

    printNamePattern(name);

    return 0;
}
