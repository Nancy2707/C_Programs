void printNamePattern(const char* name) {
    int length = strlen(name);
    
    for (int row = 0; row < ROWS; row++) {
        for (int i = 0; i < length; i++) {
            if (name[i] >= 'A' && name[i] <= 'Z') {
                int index = name[i] - 'A';
                for (int col = 0; col < COLS; col++) {
                    printf("%c", patterns[index][row][col]);
                }
            }
            printf("  "); // Add space between letters
        }
        printf("\n"); // Newline after each row of all letters
    }
}

int main() {
    char name[100];
    printf("Enter your name in uppercase: ");
    scanf("%s", name);

    printNamePattern(name);

    return 0;

}
